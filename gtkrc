{
    color["focus_line"] = "#027F01"
    bg[NORMAL] = "#027F01"
    bg[ACTIVE] = "#026002"
}

style "fixframe"
{  
    color["focus_line"] = "#FFFFFF"
    bg[NORMAL] = "#808080"
    bg[ACTIVE] = "#808080"
}

widget_class "*<GtkComboBox>*" style "mamacombo"
widget       "*ComboBox*"      style "fixframe"
